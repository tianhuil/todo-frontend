{
  "name": "todo-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "connected-react-router": "^6.5.2",
    "cuid": "^2.1.6",
    "firebase": "^7.2.0",
    "firebaseui": "^4.2.0",
    "history": "^4.10.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-firebaseui": "^4.0.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "dev-local": "REACT_APP_FIREBASE_LOCALHOST=localhost:3001 react-scripts start",
    "dev-prod": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "seed": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node -r dotenv/config -T src/batch/seedFirestore.ts",
    "serve": "firebase serve",
    "deploy": "firebase deploy",
    "deploy-firestore": "firebase deploy --only firestore",
    "firestore-local": "firebase emulators:start --only firestore",
    "test-firestore": "FIRESTORE_EMULATOR_HOST=localhost:3001 jest src/*.ts --watchAll"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@firebase/testing": "^0.14.1",
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@types/history": "^4.7.3",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/react-redux": "^7.1.4",
    "@types/react-router": "^5.1.1",
    "@types/react-router-dom": "^5.1.0",
    "dot-env": "^0.0.1",
    "firebase-tools": "^7.5.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "3.6.3"
  }
}
